<p id="notice"><%= notice %></p>

<h1>Member Grades</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @member_grades.each do |member_grade| %>
      <tr>
        <td><%= member_grade.name %></td>
        <td><%= link_to 'Show', member_grade %></td>
        <td><%= link_to 'Edit', edit_member_grade_path(member_grade) %></td>
        <td><%= link_to 'Destroy', member_grade, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member Grade', new_member_grade_path %>
